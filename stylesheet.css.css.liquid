.col-6 {width:445px;}
.col-5 {width:365px; margin-top:10px;}
.col-4 {width:285px;}
.col-3 {width:205px;}
.col-2 {width:125px;}
.col-1 {width:45px;}  

.last {margin-right:0!important}  

a.standard-button , input.standard-button, input[type="submit"]  { padding:10px; color:#0c0a5d; text-shadow:1px 1px #ddd;

background: {{settings.button_color}}; /* old browsers */

background: -moz-linear-gradient(top, {{ settings.button_color_top }} 0%, {{settings.button_color}} 100%); /* firefox */

background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,{{ settings.button_color_top }}), color-stop(100%,{{settings.button_color}})); 

border:0; font-size:19px; letter-spacing:-1px;  border:0;border-bottom:1px solid {{ settings.button_border }}; -moz-border-radius:8px; -webkit-border-radius:8px; padding:8px 10px; font-weight:bold}

a.standard-button:hover , input.standard-button:hover, input[type="submit"]:hover  { 
background: {{settings.button_color_top }}; /* old browsers */

background: -moz-linear-gradient(top, {{ settings.button_color }} 0%, {{settings.button_color_top}} 100%); /* firefox */

background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,{{ settings.button_color }}), color-stop(100%,{{settings.button_color_top}})); /* webkit */

cursor:pointer}

.cornershadow-right { background:transparent url('vig-right.png') no-repeat; position:fixed; right:0; z-index:0; width:533px; height:611px;  }
.cornershadow-left { background:transparent url('vig-left.png') no-repeat; position:fixed; left:0; z-index:0; width:533px; height:611px;  }

#wrapper { position:relative; z-index:1; padding:15px 0 0 0; }
.container { width: 925px;  

{% if settings.transparent_content_background %}
background: transparent;
{% else %}
background:{{ settings.content_background }};
-moz-box-shadow:0 0 4px #aaa; border:1px solid #ccc; border:1px rgba(0,0,0,0);
-webkit-box-shadow:0 0 4px #aaa; box-shadow:0 0 4px #aaa; 
{% endif %}

{% if settings.transparent_content_background %}
border:1px solid transparent; 
{% endif %}

margin:auto; }
.content {}


div.logo { font-weight:bold;font-size:55px; letter-spacing:-4px; float:left; color: {{settings.logo_color}}; height: 120px; }
div.logo a { color: {{settings.logo_color}};}

#header .masthead {  margin:-30px 0 0 0;}

#header .vertical-navs {
float: right;
}

#header .vertical-navs ul { float:right; list-style:none; margin:0 0 0 0; min-width:80px; }
#header .vertical-navs ul:first-child { }
#header .vertical-navs ul li{ color:#666; line-height:1.5em;}
#header .vertical-navs ul li:first-child { font-weight:bold; color:{{ settings.headings}}; margin-bottom:4px;} 
#header .vertical-navs ul a { color:#666; font-size:12px; }
#header .vertical-navs ul a:hover { color:#333; }
#header .vertical-navs ul.cart {
text-align: right;
background-color: #EEE;
padding: 5px 5px 16px 0;
border-style: dotted;
border-width: 1px;
border-color: #BBB;
text-align: right;
margin: 5px 0 0 0;
}

#header .vertical-navs ul.cart .checkout { margin-top:4px; }
#header .vertical-navs ul.cart .checkout a { background:#1571b2; -moz-border-radius:8px;-webkit-border-radius:8px;border-radius:8px;
vertical-align:middle;
background: {{settings.button_color}}; 
background: -moz-linear-gradient(top, {{ settings.button_color_top }} 0%, {{settings.button_color}} 100%); 
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,{{ settings.button_color_top }}), color-stop(100%,{{settings.button_color}})); 
padding:4px 8px 3px; color:#0c0a5d; font-size:10px; font-weight:bold; text-shadow:1px 1px  #ddd;}

#header .vertical-navs ul.cart .checkout a:hover { 
background: {{settings.button_color_top }};background: -moz-linear-gradient(top, {{ settings.button_color }} 0%, {{settings.button_color_top}} 100%); background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,{{ settings.button_color }}), color-stop(100%,{{settings.button_color_top}})); text-decoration:none;}

#header .vertical-navs ul.cart .checkout a:hover { cursor:pointer; }	
#header .vertical-navs ul.cart .checkout.inactive a { background: #F7F7F7; /* old browsers */
background: -moz-linear-gradient(top, #F7F7F7 0%, #EEE 100%); /* firefox */
background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#F7F7F7), color-stop(100%,#EEE)); /* webkit */ color:#aaa; text-shadow:none; -moz-box-shadow:0 -1px 0px #ccc; cursor:default;}
#header .vertical-navs ul.cart .checkout a:hover { cursor:pointer; }	
s{ background:-moz-linear-gradient(top, #eee, #f7f7f7 ); color:#aaa; text-shadow:none; -moz-box-shadow:0 -1px 0px #ccc;  cursor:default;}

#header ul.main-nav { margin:5px 0 30px 0; background: #444; padding: 7px 0 1px 10px; border-radius: 8px;}
#header ul.main-nav>li { float:left; list-style:none; margin-left:30px;  font-size:16px; text-transform:uppercase; font-weight: bold;}
#header ul.main-nav>li:first-child { margin-left:0;}
#header ul.main-nav>li:last-child { float:right;}
#header ul.main-nav>li>a { color: {{ settings.main_text_color }}; letter-spacing:-1px; padding:0 0 3px 0; position:relative; }
#header ul.main-nav li>a:hover { border-bottom:4px solid {{ settings.active_border }}; color: #fff; text-decoration:none; }
#header ul.main-nav>li.current a { font-weight:bold; color: #fff; }

/* drodowns */
#header ul.main-nav .dropdown-ie { position:relative; z-index:99; top:6px; -moz-top:5px; display:none} /*can't have position relative + float left on container li...intermittent documented bug*/


#header ul.main-nav ul {  margin:0; padding:0; position:absolute; left:0; background: #444;  width:230px; margin:0; padding:0 0 12px 0; }


#header li.active.dropdown a { border-bottom:4px solid  {{ settings.active_border }}; color: #fff}

#header ul.main-nav  li.dropdown { list-style-type:none; text-transform:normal;  z-index:99;}
#header ul.main-nav  li.dropdown>a { padding-right:16px;border-top:none;}

#header ul.main-nav  li.dropdown>a:after { border-top:4px solid {{ settings.main_text_color }};border-left:4px solid transparent;border-right:4px solid transparent;height:0;width:0; display:block;  content:""; position:absolute; top:7px; right:-0px; }
#header ul.main-nav  li.dropdown.active>a:after { border-top:4px solid {{ settings.active_border }};}

@-moz-document url-prefix() { .dropdown-ie { top:5px!important; background:red;;} } 
@-moz-document url-prefix() {#header ul.main-nav  li.dropdown>a:after {  top:5px; }}



#wrapper #header ul.main-nav ul li { list-style:none;}
#wrapper #header ul.main-nav ul li a { color: {{ settings.dropdown_text_color }}; display:block;  font-size:13px; text-transform:none; padding:1px 0px 8px; letter-spacing:0; margin:0 10px; border-bottom:1px solid {{ settings.dropdown_border_color }};}
#header ul.main-nav ul li a span {position:relative; top:8px; display:inline-block; padding-bottom:3px;  border-bottom:3px solid {{ settings.dropdown_border_color }}; }
#header ul.main-nav ul li a:hover span { border-bottom:3px solid {{ settings.dropdown_text_color }}; padding-bottom:3px;  }
#header ul.main-nav ul li a { text-decoration:none; }


#header ul.main-nav form { height:15px; position:relative;}
#header ul.main-nav input[type="text"] { float:left; width:150px;  line-height:15px; margin:0; vertical-align:top; border-width:2px; border-color:#888888; font-size:15px;  color:{{ settings.main_text_color }};font-family:helvetica, arial, sans-serif; text-transform:uppercase; letter-spacing:-1px; padding:2px 0 0 0;}

#header ul.main-nav .search-nav form { margin-left:-15px; border-width:2px; border-color:#888888;}

#header ul.main-nav .activesearch input[type="text"] {  border-width:2px; border-color:#888888; border-bottom:3px dotted {{ settings.active_border }}; color: #fff; float:left;   background:transparent;}

#header ul.main-nav input[type='submit'] { margin:0; padding:0;  margin-right:3px; background:   url('icon-search.png') no-repeat top left; text-indent:-9999px; width:23px; height:21px; border:0; padding:0px 0 0px 0 ;  float:left; background-position:11px 4px;cursor:pointer; }
#header ul.main-nav input[type='text']:focus { outline:none;}

{% if settings.use_dark_search_icon %}
#header ul.main-nav  input[type="submit"] {background-position:6px -60px;} 
{% endif %}

.footerbox.container { margin-top:20px; padding:0 inherit;}
