table.categories td span { display:block; border-bottom:4px solid {{ settings.light_border }}; padding: 0 0 15px 0; margin:0 0 0px 0;}



table.products { margin:30px 0 0 0;  border-collapse:collapse; clear:both; table-layout:fixed;}

table.products td { padding:0 65px 20px 0 ; }
