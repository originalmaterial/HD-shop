/* =============== */
/* = General CSS = */
/* =============== */
.clearfix:after{content:".";display:block;clear:both;visibility:hidden;line-height:0;height:0;}

a { text-decoration:none; color:{{ settings.link_color }};}
a:hover { text-decoration:underline; color:{{ settings.link_hover_color}};}

p { line-height:1.6em; margin:0 0 20px 0; color:{{ settings.text_color }};}

fieldset { padding:10px;}
li { line-height:1.3em; color:{{ settings.text_color }};}

body { background: {{ settings.color_background }} {% if settings.bg_image != "nobg" %} url( {{ settings.bg_image  | asset_url }} ) {% endif %};font-size:13px; font-family:helvetica, arial, sans-serif; color:{{ settings.text_color }}; }

blockquote { font-style:italic; font-family:georgia}
h1, h3, h4, h5, h6 { color:{{ settings.headings }}; margin-bottom:20px; margin-top:10px;}
h1 { letter-spacing:-1px; margin-bottom:20px; margin-top:0}

hr { margin-bottom:20px}

.note { background:#fffeec; padding:4px; color:#433e05; border:1px dashed #eae7c5}

input[type="text"] { padding:3px; border-width:2px; border-color:#888888;}
textarea {padding:7px; font-family:helvetica, arial, sans-serif; border:1px solid {{ settings.light_border }}; font-size:13px}
input[type="text"] { padding:7px; border:1px solid {{ settings.light_border }}; }

ul, ol { margin:0px 0 20px 20px;}

.center-text { text-align:center}

/* the grid */
